                    <button class="btn small" value="Yes" name="ynb_2323540_yes" onclick="CS.TMD.user_review_vote(2323540, 'tt1255953', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_2323540_no" onclick="CS.TMD.user_review_vote(2323540, 'tt1255953', 'no');" >No</button>
                </div>
