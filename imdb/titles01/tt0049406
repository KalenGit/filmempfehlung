                    <button class="btn small" value="Yes" name="ynb_58375_yes" onclick="CS.TMD.user_review_vote(58375, 'tt0049406', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_58375_no" onclick="CS.TMD.user_review_vote(58375, 'tt0049406', 'no');" >No</button>
                </div>
