                    <button class="btn small" value="Yes" name="ynb_1547340_yes" onclick="CS.TMD.user_review_vote(1547340, 'tt0052311', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1547340_no" onclick="CS.TMD.user_review_vote(1547340, 'tt0052311', 'no');" >No</button>
                </div>
