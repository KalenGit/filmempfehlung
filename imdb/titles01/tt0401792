                    <button class="btn small" value="Yes" name="ynb_1105824_yes" onclick="CS.TMD.user_review_vote(1105824, 'tt0401792', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1105824_no" onclick="CS.TMD.user_review_vote(1105824, 'tt0401792', 'no');" >No</button>
                </div>
