                    <button class="btn small" value="Yes" name="ynb_1686934_yes" onclick="CS.TMD.user_review_vote(1686934, 'tt0046912', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1686934_no" onclick="CS.TMD.user_review_vote(1686934, 'tt0046912', 'no');" >No</button>
                </div>
