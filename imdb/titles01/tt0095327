                    <button class="btn small" value="Yes" name="ynb_1254084_yes" onclick="CS.TMD.user_review_vote(1254084, 'tt0095327', 'yes');" >Yes</button>
                    <button class="btn small" value="No" name="ynb_1254084_no" onclick="CS.TMD.user_review_vote(1254084, 'tt0095327', 'no');" >No</button>
                </div>
